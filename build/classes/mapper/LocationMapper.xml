<mapper namespace="com.mucfc.mapper.LocationMapper">	
	<!--  查询单条记录  -->  
    <select id="findLocation" parameterType="string" resultType="Location">  
       SELECT place_lon, place_lat FROM places WHERE place_name=#{case_place}
    </select>  
